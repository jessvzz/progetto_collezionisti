<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="663.0" spacing="10.0" style="-fx-background-color: WHITE;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.collectors.controller.MySharedCollectionsController">
   <children>
      <Button fx:id="shareButton" mnemonicParsing="false" onAction="#addAction" prefHeight="31.0" prefWidth="191.0" text="Share new collection" />
      <Pane prefHeight="337.0" prefWidth="643.0">
         <children>
            <TableView fx:id="collectionsTableView" prefHeight="337.0" prefWidth="427.0">
               <columns>
                  <TableColumn fx:id="nameTableColumn" prefWidth="231.0" text="Nome della collezione" />
                  <TableColumn fx:id="idTableColumn" minWidth="0.0" prefWidth="124.0" text="ID Collezionista" />
                  <TableColumn fx:id="actionTableColumn" prefWidth="71.0" text="Manage" />
               </columns>
            </TableView>
         </children>
      </Pane>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
