<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="663.0" spacing="10.0" style="-fx-background-color: WHITE;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.collectors.controller.CollectionController">
   <children>
      <Label fx:id="nameLabel" prefHeight="21.0" prefWidth="225.0">
         <font>
            <Font name="Impact" size="24.0" />
         </font>
      </Label>
      <Button fx:id="addButton" mnemonicParsing="false" onAction="#addAction" prefHeight="31.0" prefWidth="146.0" text="Add Disk" />
      <Pane prefHeight="261.0" prefWidth="643.0">
         <children>
            <TableView fx:id="dogsTableView" layoutY="14.0" prefHeight="279.0" prefWidth="643.0">
               <columns>
                  <TableColumn fx:id="nameTableColumn" prefWidth="100.0" text="Name" />
                  <TableColumn prefWidth="383.0" text="Author" />
                  <TableColumn fx:id="editTableColumn" prefWidth="81.0" text="Edit" />
                  <TableColumn fx:id="actionTableColumn" prefWidth="78.0" text="Manage" />
               </columns>
            </TableView>
         </children>
      </Pane>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
